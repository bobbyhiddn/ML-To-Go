# AWS Credentials (for weeks 7-8)
AWS_ACCESS_KEY_ID=your_key_here
AWS_SECRET_ACCESS_KEY=your_secret_here
AWS_DEFAULT_REGION=us-east-1

# Anthropic API Key (for Claude Code)
ANTHROPIC_API_KEY=your_key_here

# HuggingFace Token (optional, for accessing gated models)
HUGGINGFACE_TOKEN=your_token_here

# SageMaker Settings (for weeks 7-8)
SAGEMAKER_EXECUTION_ROLE_ARN=arn:aws:iam::123456789012:role/SageMakerRole
SAGEMAKER_BUCKET=s3://your-sagemaker-bucket

# Model Settings
DEFAULT_EMBEDDING_MODEL=BAAI/bge-large-en-v1.5
BATCH_SIZE=32
MAX_SEQ_LENGTH=512

# Classification Settings
CONFIDENCE_THRESHOLD=0.65
LOW_CONFIDENCE_THRESHOLD=0.50
